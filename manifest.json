{
    "manifest_version": 2,
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ 	
								"synccit.user.js"
 								
							],
        "matches":          [   "http://*.reddit.com/*",
								"http://reddit.com/*",
                                "https://*.reddit.com/*",
                                "https://reddit.com/*"
                            ],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true,
    "icons":        {
            "16"    : "icon16.png",
            "48"    : "icon48.png",
            "128"   : "icon128.png"
        },
    "description":  "synccit syncs reddit.com visited links and viewed comments syncing across browsers and devices",
    "name":         "synccit for reddit",
    "version":      "12",
    "short_name":   "synccit",
	"permissions":	[	"<all_urls>"
						
		]
}